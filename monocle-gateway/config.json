{
    "name": "Monocle Gateway",
    "version": "0.1",
    "slug": "monocle-gateway",
    "description": "Monocole Gateway Add-On",
    "auto_uart": true,
    "url": "https://github.com/asmsaifs/hassio-monocle-gateway",
    "startup": "services",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "options": {
      "monocle_token": "your_very_long_token"
    },
    "schema": {
      "monocle_token": "str"
    },
    "host_network": "true",
    "map": [
      "share:rw"
    ]
}